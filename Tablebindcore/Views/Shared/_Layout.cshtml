<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tablebindcore</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Tablebindcore.styles.css" asp-append-version="true" />
 
    @* This link href  are used the style the kendo grid *@
    <!--Kendo Bundle Script to recognise kendo-->

    <script src="~/lib/kendo-ui/js/kendo.validator.min.js"></script>
    
   @* <link rel="stylesheet" href="~/KendoDynamicBind.styles.css" asp-append-version="true" />*@
   
    @* *@

	<link href="https://cdn.kendostatic.com/2022.3.1109/styles/kendo.common.min.css" rel="stylesheet" type="text/css" />
	<link href="https://cdn.kendostatic.com/2022.3.1109/styles/kendo.blueopal.min.css" rel="stylesheet" type="text/css" />
	<script src="https://cdn.kendostatic.com/2022.3.1109/js/jquery.min.js"></script>
	<script src="https://cdn.kendostatic.com/2022.3.1109/js/jszip.min.js"></script>
	<script src="https://cdn.kendostatic.com/2022.3.1109/js/kendo.all.min.js"></script>
	<script src="https://cdn.kendostatic.com/2022.3.1109/js/kendo.aspnetmvc.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Tablebindcore</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Tablebindcore - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
 @* *@
  @*  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
     @* *@

   @* <script src="~/js/site.js" asp-append-version="true"></script>*@

   @* *@

   @await RenderSectionAsync("Scripts", required: false)
     
</body>
</html>
